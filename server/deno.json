{
  "imports": {
    "hono": "jsr:@hono/hono@^4.5.3",
    "drizzle-orm": "npm:drizzle-orm@0.32.1",
    "drizzle-orm/neon-http": "npm:drizzle-orm@0.32.1/neon-http",
    "drizzle-kit": "npm:drizzle-kit@0.23.0",
    "drizzle-kit/": "npm:drizzle-kit@0.23.0/",
    "@neondatabase/serverless": "npm:@neondatabase/serverless@0.9.4"
  },
  "tasks": {
    "start": "deno run --allow-net --allow-env --allow-read main.ts",
    "dev": "deno run --allow-net --allow-env --allow-read --watch main.ts",
    "db:setup:deps": "deno cache -r --node-modules-dir npm:drizzle-orm npm:drizzle-orm/neon-http npm:drizzle-kit npm:@neondatabase/serverless",
    "db:setup:generate": "deno run -A --node-modules-dir npm:drizzle-kit generate",
    "db:generate": "deno task db:setup:deps && deno task db:setup:generate",
    "db:migrate": "deno run -A --node-modules-dir npm:drizzle-kit migrate",
    "db:seed": "deno run -A --env=.env --node-modules-dir db/scripts/seed.ts",
    "db:console": "cd db/scripts && deno repl -A --eval-file=console.ts --env=../../.env"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  },
  "deploy": {
    "project": "32eef980-ab7d-4fbc-ba56-4772ac86cb98",
    "exclude": [
      "**/node_modules"
    ],
    "include": [],
    "entrypoint": "main.ts"
  }
}